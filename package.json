{
  "name": "chesscom-themes",
  "version": "1.0.0",
  "description": "Extra themes for chessdotcom",
  "private": true,
  "scripts": {
    "clean": "rm -rf build/",
    "format": "prettier --write --ignore-unknown \"{config,app}/**/*.{html,css,js,ts,json}\"",
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "prettier": "^2.7.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
